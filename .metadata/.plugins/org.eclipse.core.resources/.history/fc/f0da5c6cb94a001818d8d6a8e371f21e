package com.homework180421prac;

public class CalTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String str = "3 + 9 / 3 + 2 * 5 * 9 - 9 / 2"; // 91.5
		
		String s[] = str.split(" ");
		
		for(int i = 0; i < s.length; i++) {
			System.out.println(s[i]);

		}

	}

}
