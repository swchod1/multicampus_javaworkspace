package com.swingex;

import java.awt.*;
import javax.swing.*;

public class DrawingPanel extends JPanel {
	
	int kor, eng, mat;
	
	@Override
	public void paint(Graphics g) {
		// TODO Auto-generated method stub
		
		g.clearRect(0, 0, getWidth(), getHeight()); // 시작점 x y , 너비, 높이
		g.drawLine(50, 250, 350, 250); // ???
		
		
	}

}
